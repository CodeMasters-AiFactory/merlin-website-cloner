{
  "name": "merlin-website-clone",
  "version": "1.0.0",
  "description": "World's #1 website cloner for complete offline backups",
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "dev": "node start-dev.js",
    "dev:old": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node dist/server/index.js",
    "start:all": "concurrently \"npm start\" \"cd frontend && npm run dev\"",
    "cli": "tsx src/cli.ts",
    "test": "vitest",
    "test:comprehensive": "tsx src/test/comprehensive-test.ts",
    "test:benchmark": "tsx src/test/benchmark-test.ts",
    "test:cloudflare": "tsx src/test/cloudflare-benchmark.ts",
    "test:cloudflare:quick": "tsx src/test/cloudflare-benchmark-quick.ts",
    "test:protection": "tsx src/test/protection-bypass-comprehensive.ts",
    "test:protection:quick": "tsx src/test/protection-bypass-comprehensive.ts quick",
    "test:protection:all": "tsx src/test/protection-bypass-comprehensive.ts all",
    "test:scale": "tsx src/test/scale-stress-test.ts",
    "test:scale:quick": "tsx src/test/scale-stress-test.ts quick",
    "test:scale:100": "tsx src/test/scale-stress-test.ts scale_100",
    "test:scale:concurrent": "tsx src/test/scale-stress-test.ts concurrent_10",
    "test:spa": "tsx src/test/spa-framework-test.ts",
    "test:spa:quick": "tsx src/test/spa-framework-test.ts quick",
    "test:spa:all": "tsx src/test/spa-framework-test.ts all",
    "test:dr": "tsx src/test/dr-comprehensive-test.ts",
    "test:warc": "tsx src/test/warc-archive-test.ts",
    "test:security": "tsx src/test/security-test.ts",
    "test:performance": "tsx src/test/performance-benchmark.ts",
    "test:all": "npm run test:security && npm run test:performance && npm run test:dr && npm run test:warc",
    "install:all": "npm install && cd frontend && npm install"
  },
  "bin": {
    "merlin-clone": "./dist/cli.js"
  },
  "keywords": [
    "website-cloner",
    "backup",
    "offline",
    "scraper"
  ],
  "author": "",
  "license": "MIT",
